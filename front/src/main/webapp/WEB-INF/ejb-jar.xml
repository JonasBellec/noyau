<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
	version="3.1">
	<enterprise-beans>
		<session>
			<ejb-name>AppConfigurationSRE</ejb-name>
			<ejb-class>fr.gouv.finances.sre.javaee6.impl.configuration.AppConfigurationSRE</ejb-class>
			<session-type>Singleton</session-type>
			<env-entry>
				<description>Nom fichier de configuration par defaut</description>
				<env-entry-name>appConfigurationFilename</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>app-configuration.properties</env-entry-value>
			</env-entry>
		</session>
	</enterprise-beans>
</ejb-jar>